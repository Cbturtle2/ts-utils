{
  "name": "@bryx911/ts-utils",
  "version": "1.6.0",
  "repository": "git@gitlab.bryx.com:bryx911/ts-utils",
  "author": "Tyler Holewinski <tyler.holewinski@bryx.com>",
  "license": "MIT",
  "private": "true",
  "scripts": {
    "build": "pika build",
    "release": "pika publish --any-branch",
    "docs": "typedoc --entryPointStrategy expand --entryPoints src/* --out public",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier -c src/*",
    "format:fix": "prettier -w src/*",
    "test:cov": "jest --coverage --runInBand",
    "version": "npm run build"
  },
  "main": "dist-node/index.js",
  "module": "dist-web/index.js",
  "types": "dist-types/index.d.ts",
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "src/**/*.test.*"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  },
  "devDependencies": {
    "@babel/preset-react": "^7.18.6",
    "@compodoc/compodoc": "^1.1.19",
    "@microsoft/api-extractor": "^7.33.7",
    "@pika/pack": "^0.5.0",
    "@pika/plugin-build-node": "^0.9.2",
    "@pika/plugin-build-types": "^0.9.2",
    "@pika/plugin-build-web": "^0.9.2",
    "@pika/plugin-standard-pkg": "^0.9.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.4",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "@ui-js/grok": "^1.8.0",
    "eslint": "^8.30.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "np": "^7.6.3",
    "prettier": "^2.8.1",
    "react-dom": "^18.2.0",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "~0.22.0",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "restricted",
    "registry": " https://gitlab.bryx.com/api/v4/projects/390/packages/npm/"
  },
  "dependencies": {
    "react": "^18.2.0"
  }
}
